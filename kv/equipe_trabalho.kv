<Equipe_trabalho>:
    FloatLayout:
        LabelButton:
            pos_hint:{'right':0.80,'top':1}
            size_hint: 0.60, 0.10
            text: 'EQUIPE DE TRABALHO:'
            font_size:17
            bold:bool

#ROTA_RADICAL
        LabelButton:
            canvas.before:
                Rectangle:
                    pos: self.pos
                    size: self.size
                    source:'icones/rota.png'
            pos_hint:{'right':0.80,'top':0.85}
            size_hint: 0.60, 0.10
            text: 'ROTA RADICAL'
            font_size:13
            bold:bool

#ROTA_51
        LabelButton:
            canvas.before:
                Rectangle:
                    pos: self.pos
                    size: self.size
                    source:'icones/rt51.png'
            pos_hint:{'right':0.80,'top':0.70}
            size_hint: 0.60, 0.10
            text: 'ROTA 51'
            font_size:13
            bold:bool


#Mudar de tela
    FloatLayout:
        pos_hint:{'right':1,'top':0.15}
        size_hint: 1, 0.15

        ImageButton:
            pos_hint:{'right':0.20,'top':0.60}
            size_hint: 0.10, 0.38
            source:'icones/3.png'
            bold: bool
            on_release: app.mudar_tela('tela1')


        ImageButton:
            pos_hint:{'right':0.90,'top':0.60}
            size_hint: 0.12, 0.40
            source:'icones/2.png'
            bold: bool

            on_release: app.mudar_tela('formularios')

        ImageButton:
            pos_hint:{'right':0.55,'top':0.60}
            size_hint: 0.12, 0.40
            source:'icones/1.png'
            bold: bool
            on_release: app.mudar_tela('tela1')


